{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/index.yml",
          "**/toc.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "Android/api-reference/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/toc.yml"
        ],
        "src": "breadcrumb",
        "dest": "rome_bread"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "ms.service": "connected-devices",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/project-rome",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "extendBreadcrumb": true,
      "uhfHeaderId": "MSDocsHeader-Windows",
      "titleSuffix": "Project Rome",
      "ms.topic": "article",
      "ms.date": "01/29/2019",
      "ms.author": "jken",
      "author": "GrantMeStrength"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "project-rome",
    "markdownEngineName": "markdig"
  }
}
